{
  "id": "com.sz84.plugin",
  "name": "sz84_plugin",
  "description": "sz84 plugin for BotNexus",
  "api_version": "1.0.0",
  "version": "1.0.0",
  "entry_point": "./sz84.plugin.exe",
  "run_on": [
    "nexus",
    "worker"
  ],
  "events": [
    "*"
  ],
  "capabilities": [
    "sz84"
  ],
  "actions": [
    "send_message"
  ],
  "permissions": [
    "send_message",
    "send_image",
    "delete_message",
    "kick_user",
    "set_group_kick",
    "mute_user",
    "set_group_ban",
    "leave_group",
    "set_group_leave",
    "set_group_card",
    "set_group_name",
    "set_group_special_title",
    "set_group_admin",
    "set_group_whole_ban",
    "get_group_info",
    "get_group_member_info",
    "storage.get",
    "storage.set",
    "storage.delete",
    "storage.exists"
  ],
  "timeout_ms": 5000,
  "max_concurrency": 10,
  "max_restarts": 3,
  "signature": "",
  "plugin_level": "feature",
  "source": "internal"
}
