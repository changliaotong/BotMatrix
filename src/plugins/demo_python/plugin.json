{
  "name": "demo_python",
  "description": "Python SDK 3.0 Demo Plugin",
  "api_version": "1.0.0",
  "version": "1.0.0",
  "entry_point": "python main.py",
  "run_on": ["worker"],
  "capabilities": ["interactive_dialog", "async_tasks"],
